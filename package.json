{
  "name": "my-new-app",
  "productName": "my-new-app",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Khiem Luc"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "custom_report"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/electronjs/index.html",
                  "js": "./src/main/renderer.jsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.10.1",
    "@electron-forge/cli": "^6.0.0-beta.51",
    "@electron-forge/maker-deb": "^6.0.0-beta.51",
    "@electron-forge/maker-rpm": "^6.0.0-beta.51",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.51",
    "@electron-forge/maker-zip": "^6.0.0-beta.51",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.51",
    "@kintone/kintone-js-sdk": "^0.4.0",
    "@kintone/rest-api-client": "^1.4.1",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "antd": "^4.3.1",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "core-js": "^2.6.11",
    "css-loader": "^3.5.3",
    "electron": "9.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.21.2",
    "node-loader": "^0.6.0",
    "react-hot-loader": "^4.3.5",
    "react-router-dom": "^4.3.1",
    "recharts": "^1.2.0",
    "style-loader": "^0.23.1"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "kintone-test-preparation": "file:../data-generator-server",
    "raw-loader": "^4.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
